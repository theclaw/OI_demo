<dashboard script="e2e.js">
  <label>Environment State</label>
  <description/>
  <row>
    <html>
      <div id="e2e"/>
    </html>
  </row>
  <row grouping="6,6,6">
    <chart>
      <title>Key Web Server Metrics</title>
      <searchString>index=oidemo * | head 1 | eval count = 20 | gauge count 0 25 50 75 100</searchString>
      <earliestTime>-1m@m</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.chart">radialGauge</option>
      <option name="height">150px</option>
      <option name="charting.seriesColors">[0x5379af, 0x9ac23c, 0xf7902b, 0x956d95, 0x6ab7c7, 0xd85d3c, 0xfac51c, 0xdd86af]</option>
      <option name="charting.legend.masterLegend">null</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.rangeValues">["0","30","70","100"]</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">minimal</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.gaugeColors">[0x84E900,0xFFE800,0xBF3030]</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <html>
      <center>Average Transaction Duration</center>
    </html>
    <chart>
      <searchString>index=oidemo sourcetype="access_combined" | timechart avg(timetaken) by src</searchString>
      <earliestTime>-60m@m</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.chart">line</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisEnd</option>
      <option name="charting.axisTitleY.text">ms</option>
      <option name="charting.axisTitleX.text"/>
      <option name="chart.nullValueMode">connect</option>
      <option name="height">150px</option>
      <option name="charting.seriesColors">[0x5379af, 0x9ac23c, 0xf7902b, 0x956d95, 0x6ab7c7, 0xd85d3c, 0xfac51c, 0xdd86af]</option>
      <option name="charting.legend.masterLegend">null</option>
    </chart>
    <html>
      <hr/>
    </html>
    <table>
      <title>Top status codes (Last 15m)</title>
      <searchString>index=oidemo sourcetype="access_combined" | stats count(status) as count sparkline(count) as sparkline by status_description | rename status_description as "http status code" | sort - count</searchString>
      <earliestTime>-15m@m</earliestTime>
      <latestTime>now</latestTime>
      <option name="entityName">auto</option>
      <option name="displayRowNumbers">off</option>
      <option name="dataOverlayMode">heatmap</option>
      <option name="wrap">false</option>
      <format type="sparkline" field="sparkline">
        <option name="lineColor">#5379af</option>
        <option name="fillColor">#CCDDFF</option>
        <option name="lineWidth">1</option>
        <option name="height">20</option>
      </format>
    </table>
    <html>
      <a href="#">Inspect web servers</a>...</html>
    <chart>
      <title>Key App Server Metrics</title>
      <searchString>index=oidemo sourcetype="access_combined" | eval capacity = bytes / 50 | stats avg(capacity) as capacity | eval rcapacity = round(capacity) | gauge rcapacity 0 25 50 75 200</searchString>
      <earliestTime>rt-2m</earliestTime>
      <latestTime>rt</latestTime>
      <option name="charting.chart">radialGauge</option>
      <option name="height">150px</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.rangeValues">["0","30","70","100"]</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">minimal</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.gaugeColors">[0x84E900,0xFFE800,0xBF3030]</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <html>
      <center>Average Transaction Duration</center>
    </html>
    <chart>
      <searchString>index=oidemo sourcetype="access_combined" | eval timetaken = timetaken / 6 | timechart avg(timetaken) by src</searchString>
      <earliestTime>-60m@m</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.chart">line</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisEnd</option>
      <option name="charting.axisTitleY.text">ms</option>
      <option name="chart.nullValueMode">connect</option>
      <option name="height">150px</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.seriesColors">[0x5379af, 0x9ac23c, 0xf7902b, 0x956d95, 0x6ab7c7, 0xd85d3c, 0xfac51c, 0xdd86af]</option>
      <option name="charting.legend.masterLegend">null</option>
    </chart>
    <html>
      <hr/>
    </html>
    <chart>
      <title>JVM Heap Size</title>
      <searchString>index=oidemo sourcetype="access_combined" | eval bytes = bytes / 3 | timechart avg(bytes) by src</searchString>
      <earliestTime>-60@m</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.chart">area</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisEnd</option>
      <option name="charting.axisTitleY.text">megabytes</option>
      <option name="charting.axisTitleX.text"/>
      <option name="chart.nullValueMode">connect</option>
      <option name="charting.seriesColors">[0x5379af, 0x9ac23c, 0xf7902b, 0x956d95, 0x6ab7c7, 0xd85d3c, 0xfac51c, 0xdd86af]</option>
      <option name="charting.legend.masterLegend">null</option>
    </chart>
    <html>
      <a href="#">Inspect application servers</a>...</html>
    <chart>
      <title>Key Database Metrics</title>
      <searchString>index=oidemo sourcetype="access_combined" | eval capacity = bytes / 30 | stats avg(capacity) as capacity | eval rcapacity = round(capacity) | gauge rcapacity 0 250 500 750 1000</searchString>
      <earliestTime>rt-2m</earliestTime>
      <latestTime>rt</latestTime>
      <option name="charting.chart">radialGauge</option>
      <option name="height">150px</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.rangeValues">["0","30","70","100"]</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">minimal</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.gaugeColors">[0x84E900,0xFFE800,0xBF3030]</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
    <html>
      <center>Average Transaction Duration</center>
    </html>
    <chart>
      <searchString>index=oidemo sourcetype="mysql_perf" | timechart avg(transaction_speed) by src</searchString>
      <earliestTime>-60m@m</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.chart">line</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisEnd</option>
      <option name="charting.axisTitleY.text">ms</option>
      <option name="charting.axisTitleX.text"/>
      <option name="chart.nullValueMode">connect</option>
      <option name="height">150px</option>
      <option name="charting.seriesColors">[0x5379af, 0x9ac23c, 0xf7902b, 0x956d95, 0x6ab7c7, 0xd85d3c, 0xfac51c, 0xdd86af]</option>
      <option name="charting.legend.masterLegend">null</option>
    </chart>
    <html>
      <hr/>
    </html>
    <chart>
      <title>Database Concurrency</title>
      <searchString>index=oidemo sourcetype=mysql_perf  | rex "(?:(FROM\s+|INTO\s+)(?&lt;table&gt;\w+))"  | eval table=lower(table) | eval duration=round(transaction_speed/1000, 0) | concurrency duration=duration | timechart max(concurrency) as "Max Concurrency" | eval "Expected Concurrency"=1</searchString>
      <earliestTime>-60m@m</earliestTime>
      <latestTime>now</latestTime>
      <option name="charting.axisTitleX.visibility">collapsed</option>
      <option name="charting.axisTitleY.text">Query</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">zero</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.seriesColors">[0x5379af, 0x9ac23c, 0xf7902b, 0x956d95, 0x6ab7c7, 0xd85d3c, 0xfac51c, 0xdd86af]</option>
      <option name="charting.legend.masterLegend">null</option> 
    </chart>
    <html>
      <a href="db_state">Inspect database servers</a>...</html>
  </row>
</dashboard>